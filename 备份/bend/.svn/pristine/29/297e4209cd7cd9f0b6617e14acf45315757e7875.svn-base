<A NAME="SQL"></A>
<TABLE CLASS="SUB_TABLE"><TR><TD CLASS="SUB_TITLE">
SQL
</TD></TR></TABLE><PRE CLASS="DECL_TEXT">
create or replace view tcs_v_rep_finish_ots as
select ots.id_site
      ,ots.id_system
      ,ots.id_order
      ,ots.num_target
      ,ots.num_step
      ,ots.num_barcode
      ,ord.priority
      ,ord.id_lu
      ,ord.typ_lu
      ,ots.storage_area_src
      ,ots.storage_location_src
      ,ots.id_node_src
      ,ots.storage_area_tgt
      ,ots.storage_location_tgt
      ,ots.id_node_tgt
  from tcs_order_target_step ots
      , tcs_order ord
 where ots.id_site = ord.id_site
   and ots.id_system = ord.id_system
   and ots.id_order = ord.id_order;
</PRE>
