<TABLE CLASS="SUB_TABLE"><TR><TD CLASS="SUB_TITLE">
SQL
</TD></TR></TABLE><PRE CLASS="DECL_TEXT">
create or replace view stm_v_stock_taking_discovery as
select
       r.id_site,
       r.id_ref_2 as id_stock_taking_list,
       r.id_record,
       r.storage_area_src,
       r.storage_location_src,
       r.id_lu_src as id_lu,
       c.typ_lu_discovery as typ_lu,
       r.id_client,
       a.id_article,
       a.txt_label,
       case when slt.typ_remaining_capacity = 'CNTLU' then slt.typ_remaining_capacity else a.unit_stock end as unit_stock,
       r.qty,
       r.typ_special_stock,
       r.id_special_stock,
       r.id_batch,
       r.ts_bbd,
       r.sep_crit_01,
       r.sep_crit_02,
       r.sep_crit_03,
       r.sep_crit_04,
       r.sep_crit_05,
       r.sep_crit_06,
       r.sep_crit_07,
       r.sep_crit_08,
       r.sep_crit_09,
       r.sep_crit_10,
       r.artvar,
       r.typ_stock,
       r.typ_lock,
       r.stat_quality_control,
       r.stat_customs,
       r.stat_step as stat,
       c.txt_count,
       c.height_discovery
from bps_process_record r
left join art_article a on a.id_client = r.id_client and a.id_article = r.id_article
left join top_storage_location sl on sl.id_site = r.id_site and sl.storage_area = r.storage_area_src and sl.storage_location = r.storage_location_src
left join top_storage_location_type slt on slt.id_site = sl.id_site and slt.typ_storage_location = sl.typ_storage_location
join stm_stock_taking_count c on c.id_site = r.id_site and c.id_record = r.id_record and c.cnt_count = (select max(sub.cnt_count) from stm_stock_taking_count sub where sub.id_site = r.id_site and sub.id_record = r.id_record)
where
      r.typ_ref = 'STOCK_TAKING_LIST'
order by
      r.id_site,
      r.id_record asc;
</PRE>
