<TABLE CLASS="SUB_TABLE"><TR><TD CLASS="SUB_TITLE">
SQL
</TD></TR></TABLE><PRE CLASS="DECL_TEXT">
create or replace view im_v_rep_occupancy_list as
with stock_data as (select qua.id_site,
   -----------------------------------------------------------------------------
   -- Component   : WMS_INVENTORY
   -- HEAD URL    : $URL: svn://ifbsrc1/Bend2.0/IWFComponent/releases/10_wms/wms_inventory/snapshot/comp/inst_components/db/dbobj/views/im_v_rep_occupancy_list.vew $
   -- Revision    : $Rev: 31250 $
   -- Changed     : $Id: im_v_rep_occupancy_list.vew 31250 2019-05-22 15:33:42Z sfarsad-assadi $
   -- Changed from: $Author: sfarsad-assadi $
   -- Copyrights  : (C) inconso AG, Germany
   -- Description : View to select data for report
   -----------------------------------------------------------------------------
                           qua.id_client,
                               qua.id_article,
                               qua.id_batch,
                               qua.typ_stock,
                               qua.typ_lock,
                               qua.ts_bbd,
                               qua.id_lu_1,
                               qua.typ_lu_1,
                               qua.storage_area,
                               qua.storage_location,
                               sum (qua.qty_available + qua.qty_reserved) as sumqty
                          from im_quantum qua
                         group by qua.id_site,
                                  qua.id_client,
                                  qua.id_article,
                                  qua.id_batch,
                                  qua.typ_stock,
                                  qua.typ_lock,
                                  qua.ts_bbd,
                                  qua.id_lu_1,
                                  qua.typ_lu_1,
                                  qua.storage_area,
                                  qua.storage_location)
select scl.id_site,
       scl.id_client,
       scl.id_article,
       scl.id_locale,
       scl.txt_label,
       scl.id_batch,
       scl.sumqty,
       scl.unit_stock,
       scl.typ_stock,
       scl.typ_lock,
       scl.ts_bbd,
       COALESCE(scl.id_lu_1, '---') as id_lu_1,
       scl.typ_lu_1,
       scl.storage_area,
       scl.storage_location,
       scl.stat_occupied,
       scl.pct_occupied
 from  im_v_rep_stock_control_list scl
 order by id_site, storage_area, storage_location,id_lu_1, id_client, id_article, id_batch
;
