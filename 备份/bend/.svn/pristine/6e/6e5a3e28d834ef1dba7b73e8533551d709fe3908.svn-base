<TABLE CLASS="SUB_TABLE"><TR><TD CLASS="SUB_TITLE">
SQL
</TD></TR></TABLE><PRE CLASS="DECL_TEXT">
create or replace view pi_v_rep_picking_order_list as
select po.id_site
   -----------------------------------------------------------------------------
   -- Component   : WMS_PICKING
   -- HEAD URL    : $URL: svn://ifbsrc1/Bend2.0/IWFComponent/development/10_wms/wms_picking/trunk/Component/db/dbobj/views/pi_v_rep_picking_order_list.vew $
   -- Revision    : $Rev: 33704 $
   -- Changed     : $Id: pi_v_rep_picking_order_list.vew 33704 2019-07-09 07:41:19Z swerda $
   -- Changed from: $Author: swerda $
   -- Copyrights  : (C) inconso AG, Germany
   -- Description : View to select data for report
   -----------------------------------------------------------------------------
        ,po.id_picking_trip
        ,po.id_picking_order
        ,po.id_store_out_zone
        ,po.ts_release
        ,poa.id_pick
        ,pr.id_client
        ,pr.id_article
        ,pr.qty
        ,pr.id_batch
        ,pr.ts_bbd
        ,art.txt_label
        ,art.unit_stock
   from pi_picking_order            po
join pi_picking_order_bps_asgmt poa on poa.id_site = po.id_site and poa.id_picking_order = po.id_picking_order
join bps_process_record         pr  on pr.id_site = poa.id_site and pr.id_record = poa.id_record
join art_article                art on art.id_client = pr.id_client and art.id_article = pr.id_article
order by
   pr.storage_area_src
  ,pr.storage_location_src
  ,pr.id_article
  ,poa.id_pick
;
</PRE>
