<A NAME="SQL"></A>
<TABLE CLASS="SUB_TABLE"><TR><TD CLASS="SUB_TITLE">
SQL
</TD></TR></TABLE><PRE CLASS="DECL_TEXT">
create or replace view stm_v_stock_taking_item as
select
       i.id_site,
       i.id_stock_taking_list,
       i.num_item,
       r.id_record,
       i.stat,
       i.storage_area,
       i.storage_location,
       l.id_lu,
       l.typ_lu,
       r.id_client,
       a.id_article,
       a.txt_label,
       case when i.typ_item = 'BULK' then 'CNTLU' else a.unit_stock end as unit_stock,
       r.qty as qty_target,
       c.qty_actual,
       c.qty_actual - r.qty as qty_difference,
       c_last.qty_actual as qty_last_count,
       c.cnt_count,
       case when c.qty_actual != r.qty then '1' else '0' end as flg_recount,
       r.typ_special_stock,
       r.id_special_stock,
       r.id_batch,
       r.ts_bbd,
       r.sep_crit_01,
       r.sep_crit_02,
       r.sep_crit_03,
       r.sep_crit_04,
       r.sep_crit_05,
       r.sep_crit_06,
       r.sep_crit_07,
       r.sep_crit_08,
       r.sep_crit_09,
       r.sep_crit_10,
       r.artvar,
       r.typ_stock,
       r.typ_lock,
       r.stat_quality_control,
       r.stat_customs,
       r.typ_step,
       r.stat_step
from stm_stock_taking_item i
join bps_process_record r on
     r.typ_ref = 'STOCK_TAKING_ITEM'
 and r.id_ref_1 = i.id_site
 and r.id_ref_2 = i.id_stock_taking_list
 and r.id_ref_3 = i.num_item
 and r.id_ref_4 is null
 and r.id_ref_5 is null
 and r.id_ref_6 is null
left join im_load_unit l on l.id_site = r.id_site and l.id_lu = r.id_lu_src
left join art_article a on a.id_client = r.id_client and a.id_article = r.id_article
join stm_stock_taking_count c on c.id_site = r.id_site and c.id_record = r.id_record and c.cnt_count = (select max(sub.cnt_count) from stm_stock_taking_count sub where sub.id_site = r.id_site and sub.id_record = r.id_record)
left join stm_stock_taking_count c_last on c_last.id_site = r.id_site and c_last.id_record = r.id_record and c_last.cnt_count = c.cnt_count - 1
order by
       r.id_site,
       r.id_record asc;
</PRE>
