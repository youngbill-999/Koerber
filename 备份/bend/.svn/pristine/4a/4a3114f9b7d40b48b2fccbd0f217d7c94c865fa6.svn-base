<A NAME="SQL"></A>
<TABLE CLASS="SUB_TABLE"><TR><TD CLASS="SUB_TITLE">
SQL
</TD></TR></TABLE><PRE CLASS="DECL_TEXT">
create or replace view v_hoi_trans_pos_out as
select
t1.num_trans as num_trans
,posout."TYPE" as type
,posout."NUM_DATABLOCK" as num_datablock
,posout."NUM_POS" as num_pos
,posout."ID_RECORD_TYPE"
,posout."DATA"
,posout."KEY_1"
,posout."KEY_2"
,posout."KEY_3"
,posout."KEY_4"
,posout."KEY_5"
,posout."KEY_6"
,posout."STAT"
,posout."TS_CRE"
,posout."ID_CRE"
,posout."TS_UPD"
,posout."ID_UPD"
from hoi_trans t1
LEFT JOIN hoi_trans t2
     on t1.num_trans = t2.num_trans_agg
JOIN hoi_trans_pos_out posout
     on t2.num_trans = posout.num_trans
UNION ALL
select
t1.num_trans as num_trans
,posout."TYPE" as type
,posout."NUM_DATABLOCK" as num_datablock
,posout."NUM_POS" as num_pos
,posout."ID_RECORD_TYPE"
,posout."DATA"
,posout."KEY_1"
,posout."KEY_2"
,posout."KEY_3"
,posout."KEY_4"
,posout."KEY_5"
,posout."KEY_6"
,posout."STAT"
,posout."TS_CRE"
,posout."ID_CRE"
,posout."TS_UPD"
,posout."ID_UPD"
from hoi_trans t1
LEFT JOIN hoi_trans t2
     on t1.num_trans = t2.num_trans_agg
JOIN hoi_trans_pos_out posout
     on t1.num_trans = posout.num_trans
order by num_trans,TYPE,NUM_DATABLOCK,NUM_POS;
</PRE>
