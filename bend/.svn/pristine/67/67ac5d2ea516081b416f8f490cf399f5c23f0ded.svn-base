create or replace view res_v_quantum as
select pr.id_site,
-----------------------------------------------------------------------------
-- Component  : WMS_RESERV
-- HEAD URL   : $URL: svn://ifbsrc1/Bend2.0/IWFComponent/releases/10_wms/wms_reserv/snapshot/comp/inst_components/db/dbobj/views/res_v_quantum.vew $
-- Revision   : $Rev: 40567 $
-- Change at  : $Id: res_v_quantum.vew 40567 2019-10-07 14:19:24Z ajulien $
-- Change by  : $Author: ajulien $
-- Copyrights : (C) inconso AG, Germany
-- Description: view for RES900
-----------------------------------------------------------------------------
       pr.id_record,
       qd.id_quantum,
       qd.id_client,
       qd.id_article,
       art.unit_stock,
       qd.qty_available,
       qd.qty_reserved,
       qd.id_lu_1,
       qd.typ_lu_1,
       qd.storage_area,
       qd.storage_location,
       qd.flg_in_transit,
       qd.aisle,
       decode(qd.zone_active, '1', qd.id_store_out_zone, null) as id_store_out_zone,
       decode(qd.asgmt_active, '1', decode(qd.group_active, '1', qd.id_store_out_group)) as id_store_out_group,
       qd.qty_per_lu                                                                                      qty_unopened,
       qd.typ_lu_unopened                                                                                 typ_unopened,
       -- BestKrit
       decode(pr.artvar,              null,'0',qd.artvar,              '0','1')                           flg_artvar,                  -- V
       decode(pr.typ_stock,           null,'0',qd.typ_stock,           '0','1')                           flg_typ_stock,               -- B
       decode(pr.typ_lock,            null,'0',qd.typ_lock,            '0','1')                           flg_typ_lock,                -- S
       decode(pr.typ_special_stock,   null,'0',qd.typ_special_stock,   '0','1')                           flg_typ_special_stock,       -- T
       decode(pr.id_special_stock,    null,'0',qd.id_special_stock,    '0','1')                           flg_id_special_stock,        -- I
       decode(pr.stat_quality_control,null,'0',qd.stat_quality_control,'0','1')                           flg_stat_quality_control,    -- Q
       decode(pr.stat_customs,        null,'0',qd.stat_customs,        '0','1')                           flg_stat_customs,            -- Z
       decode(pr.id_batch,            null,'0',qd.id_batch,            '0','1')                           flg_id_batch,                -- C
       decode(pr.ts_bbd,              null,'0',qd.ts_bbd,              '0','1')                           flg_ts_bbd,                  -- M
       -- TrennKrit
       decode(qd.flg_sep_crit_01_nullable,'0',decode(pr.sep_crit_01,null,'0',qd.sep_crit_01,'0','1'),
                                          '1',decode(pr.sep_crit_01,         qd.sep_crit_01,'0','1'),'1') flg_01,                      -- 1
       decode(qd.flg_sep_crit_02_nullable,'0',decode(pr.sep_crit_02,null,'0',qd.sep_crit_02,'0','1'),
                                          '1',decode(pr.sep_crit_02,         qd.sep_crit_02,'0','1'),'1') flg_02,                      -- 2
       decode(qd.flg_sep_crit_03_nullable,'0',decode(pr.sep_crit_03,null,'0',qd.sep_crit_03,'0','1'),
                                          '1',decode(pr.sep_crit_03,         qd.sep_crit_03,'0','1'),'1') flg_03,                      -- 3
       decode(qd.flg_sep_crit_04_nullable,'0',decode(pr.sep_crit_04,null,'0',qd.sep_crit_04,'0','1'),
                                          '1',decode(pr.sep_crit_04,         qd.sep_crit_04,'0','1'),'1') flg_04,                      -- 4
       decode(qd.flg_sep_crit_05_nullable,'0',decode(pr.sep_crit_05,null,'0',qd.sep_crit_05,'0','1'),
                                          '1',decode(pr.sep_crit_05,         qd.sep_crit_05,'0','1'),'1') flg_05,                      -- 5
       decode(qd.flg_sep_crit_06_nullable,'0',decode(pr.sep_crit_06,null,'0',qd.sep_crit_06,'0','1'),
                                          '1',decode(pr.sep_crit_06,         qd.sep_crit_06,'0','1'),'1') flg_06,                      -- 6
       decode(qd.flg_sep_crit_07_nullable,'0',decode(pr.sep_crit_07,null,'0',qd.sep_crit_07,'0','1'),
                                          '1',decode(pr.sep_crit_07,         qd.sep_crit_07,'0','1'),'1') flg_07,                      -- 7
       decode(qd.flg_sep_crit_08_nullable,'0',decode(pr.sep_crit_08,null,'0',qd.sep_crit_08,'0','1'),
                                          '1',decode(pr.sep_crit_08,         qd.sep_crit_08,'0','1'),'1') flg_08,                      -- 8
       decode(qd.flg_sep_crit_09_nullable,'0',decode(pr.sep_crit_09,null,'0',qd.sep_crit_09,'0','1'),
                                          '1',decode(pr.sep_crit_09,         qd.sep_crit_09,'0','1'),'1') flg_09,                      -- 9
       decode(qd.flg_sep_crit_10_nullable,'0',decode(pr.sep_crit_10,null,'0',qd.sep_crit_10,'0','1'),
                                          '1',decode(pr.sep_crit_10,         qd.sep_crit_10,'0','1'),'1') flg_10,                      -- 0
       -- Platz
       qd.loc_flg_block_conv_tec                                                                          flg_loc_block_conv_tec,      -- F
       qd.loc_flg_block_store_out                                                                         flg_loc_block_store_out,     -- E
       -- Gang
       decode(qd.aisle_flg_block_store_out,  null,'0',qd.aisle_flg_block_store_out)                       flg_aisle_block_store_out,   -- E
       decode(qd.aisle_flg_block_disposition,null,'0',qd.aisle_flg_block_disposition)                     flg_aisle_block_disposition, -- D
       -- Sonstiges
       decode(qd.art_stat,'00','1','0')                                                                   flg_art_stat,                -- A
       greatest(qd.quant_flg_stock_taking,qd.loc_flg_stock_taking)                                        flg_stock_taking,            -- I
       qd.area_flg_virtual                                                                                flg_area_virtual,            -- V
       decode(qd.id_store_out_zone,null,'1','0')                                                          flg_id_store_out_zone,       -- Z
       decode(qd.id_store_out_group,null,'1','0')                                                         flg_id_store_out_group,      -- G
       -- PK IWF
       pr.id_site||'@'||
       pr.id_record||'@'||
       qd.id_quantum||'@'||
       qd.id_store_out_zone||'@'||
       qd.id_store_out_group||'@'||
       to_char(qd.qty_per_lu)||'@'||
       qd.typ_lu_unopened||'@'                                                                            pk
  from bps_process_record pr,
       res_v_quantum_detail qd,
       art_article art
 where pr.typ_step    = 'RESERV'
   and qd.id_site     = pr.id_site
   and qd.id_client   = pr.id_client
   and qd.id_article  = pr.id_article
   and art.id_client  = qd.id_client
   and art.id_article = qd.id_article
;
