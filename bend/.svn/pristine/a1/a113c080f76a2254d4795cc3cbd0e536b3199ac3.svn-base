<A NAME="SQL"></A>
<TABLE CLASS="SUB_TABLE"><TR><TD CLASS="SUB_TITLE">
SQL
</TD></TR></TABLE><PRE CLASS="DECL_TEXT">
create or replace view art_v_pu_lu_type as
select
       pack1.id_site,
-----------------------------------------------------------------------------
-- Komponente     : WMS_ARTICLE
-- HEAD URL       : $URL: svn://ifbsrc1/Bend2.0/IWFComponent/releases/10_wms/wms_article/snapshot/comp/inst_components/db/dbobj/views/art_v_pu_lu_type.vew $
-- Revision       : $Rev: 31866 $
-- Aenderung am   : $Id: art_v_pu_lu_type.vew 31866 2019-06-11 12:01:34Z sloesch $
-- Aenderung durch: $Author: sloesch $
-- Copyrights     : (C) inconso AG, Germany
-- Beschreibung   : View to select quantity information of packing units / formally known as v_we230_letypen
-----------------------------------------------------------------------------
       pack1.id_client,
       pack1.id_article,
       pack1.typ_lu,
       pack1.artvar,
       pack1.layer,
       pack1.qty_per_lu,
       CASE
         WHEN pack1.layer = '1' then trunc(pack1.qty_per_lu/pack2.qty_per_lu)
         ELSE null
       END as count2,
       CASE
         WHEN pack1.layer = '1' then trunc(pack2.qty_per_lu/pack3.qty_per_lu)
         WHEN pack1.layer = '2' then trunc(pack1.qty_per_lu/pack2.qty_per_lu)
         ELSE null
       END as  count3,
       coalesce(pack3.qty_per_lu,pack2.qty_per_lu,pack1.qty_per_lu) qty_mu,
       --only for heightclasses
       pack1.height,
       pack1.flg_shippable,
       pack1.id_cre,
       pack1.ts_cre,
       pack1.id_upd,
       pack1.ts_upd
  from
       art_art_packing_unit pack1,
       art_art_packing_unit pack2,
       art_art_packing_unit pack3
 where pack2.id_site(+)    = pack1.id_site
   and pack2.id_client(+)  = pack1.id_client
   and pack2.id_article(+) = pack1.id_article
   and pack2.artvar(+)     = pack1.artvar
   and pack2.layer(+)      = pack1.layer + 1
   and pack3.id_site(+)    = pack2.id_site
   and pack3.id_client(+)  = pack2.id_client
   and pack3.id_article(+) = pack2.id_article
   and pack3.artvar(+)     = pack2.artvar
   and pack3.layer(+)      = pack2.layer + 1
;
</PRE>
