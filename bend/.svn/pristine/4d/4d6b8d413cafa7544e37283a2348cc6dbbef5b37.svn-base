       pr.TYP_REF,
       pr.ID_REF_1,
       pr.ID_REF_2,
       pr.ID_REF_3,
       pr.ID_REF_4,
       pr.ID_REF_5,
       pr.ID_REF_6,
       /*Reiter:Bezug/Eigenschaften*/
       pr.ARTVAR,
       pr.TYP_STOCK,
       pr.TYP_LOCK,
       pr.TYP_SPECIAL_STOCK,
       pr.ID_SPECIAL_STOCK,
       pr.STAT_QUALITY_CONTROL,
       pr.STAT_CUSTOMS,
       pr.ID_BATCH,
       pr.TS_BBD,
