<A NAME="SQL"></A>
<TABLE CLASS="SUB_TABLE"><TR><TD CLASS="SUB_TITLE">
SQL
</TD></TR></TABLE><PRE CLASS="DECL_TEXT">
create or replace view im_v_rep_stock_list as
with stock_data   as (select qua.id_site,
   -----------------------------------------------------------------------------
   -- Component   : WMS_INVENTORY
   -- HEAD URL    : $URL: svn://ifbsrc1/Bend2.0/IWFComponent/development/10_wms/wms_inventory/trunk/Component/db/dbobj/views/im_v_rep_stock_list.vew $
   -- Revision    : $Rev: 27237 $
   -- Changed     : $Id: im_v_rep_stock_list.vew 27237 2019-03-26 12:02:17Z wlanger $
   -- Changed from: $Author: wlanger $
   -- Copyrights  : (C) inconso AG, Germany
   -- Description : View to select data for report
   -----------------------------------------------------------------------------
                               qua.id_client,
                               qua.id_article,
                               qua.id_batch,
                               qua.typ_stock,
                               qua.typ_lock,
                               qua.ts_bbd,
                               sum(qua.qty_available) as qty_available,
                               sum(qua.qty_reserved)  as qty_reserved
                          from im_quantum qua
                         group by qua.id_site,
                                  qua.id_client,
                                  qua.id_article,
                                  qua.id_batch,
                                  qua.typ_stock,
                                  qua.typ_lock,
                                  qua.ts_bbd)
select stc.id_site,
       art.id_client,
       art.id_article,
       ads.id_locale,
       ads.txt_label,
       stc.id_batch,
       stc.qty_available,
       stc.qty_reserved,
       art.unit_stock,
       stc.typ_stock,
       stc.typ_lock,
       stc.ts_bbd
  from stock_data      stc,
       art_article     art,
       art_v_art_label ads
 where art.id_client  = stc.id_client
   and art.id_article = stc.id_article
   and ads.id_client  = art.id_client
   and ads.id_article = art.id_article
;
