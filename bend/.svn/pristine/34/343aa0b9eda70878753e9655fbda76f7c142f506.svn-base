<TABLE CLASS="SUB_TABLE"><TR><TD CLASS="SUB_TITLE">
SQL
</TD></TR></TABLE><PRE CLASS="DECL_TEXT">
create or replace view gr_v_rep_gr_list as
select gre.id_site,
   -----------------------------------------------------------------------------
   -- Component   : WMS_GRBASE
   -- HEAD URL    : $URL: svn://ifbsrc1/Bend2.0/IWFComponent/releases/10_wms/wms_grbase/snapshot/comp/inst_components/db/dbobj/views/gr_v_rep_gr_list.vew $
   -- Revision    : $Rev: 31277 $
   -- Changed     : $Id: gr_v_rep_gr_list.vew 31277 2019-05-23 11:15:43Z sfarsad-assadi $
   -- Changed from: $Author: sfarsad-assadi $
   -- Copyrights  : (C) inconso AG, Germany
   -- Description : View to select data for report
   -----------------------------------------------------------------------------
       gre.id_client,
       gre.id_goods_receipt,
       gri.num_item,
       gri.id_batch,
       gri.ts_bbd,
       gri.qty_target + gri.qty_actual as sumqty,
       gdi.id_ref_external_1,
       gdi.id_ref_external_2,
       gdi.id_ref_external_item,
       grd.id_inbound_delivery,
       grd.num_delivery_note,
       grd.dat_delivery_note,
       art.id_article,
       art.unit_stock,
       ps.id_supplier,
       ads.id_locale,
       ads.txt_label
  from gr_receiving             gre,
       gr_receiving_item        gri,
       gr_inbound_delivery_item gdi,
       gr_inbound_delivery      grd,
       art_article              art,
       pm_supplier              ps,
       art_v_art_label          ads
 where
      --gr_receiving/gr_receinivg_item
      gre.id_site          = gri.id_site
  and gre.id_client        = gri.id_client
  and gre.id_goods_receipt = gri.id_goods_receipt
      --gr_receiving_item/gr_inbound_delivery_item
  and gri.id_site          = gdi.id_site
  and gri.id_client        = gdi.id_client
  and gri.num_item         = gdi.num_item
      --gr_inbound_delivery_item/gr_inbound_delivery
  and gdi.id_site          = grd.id_site
  and gdi.id_client        = grd.id_client
  and gdi.id_inbound_delivery = grd.id_inbound_delivery
      --gr_receiving/art_article
  and gre.id_client        = art.id_client
      --art_article/pm_supplier
  and art.id_client        = ps.id_client
      --art_v_art_label/art_article
  and ads.id_client        = art.id_client
  and ads.id_article       = art.id_article
      --gr_receiving/pm_supplier
  and gre.id_supplier        = ps.id_supplier
  and gre.id_client          = ps.id_client
;
</PRE>
