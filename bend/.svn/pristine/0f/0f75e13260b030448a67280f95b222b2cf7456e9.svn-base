-- Beschreibung   : View to select processing items in picking (in the processing steps CLASSIFY and PICKING):
-----------------------------------------------------------------------------
       pia.ID_RECORD as ID_RECORD ,
       pia.id_pick,
       pr.TYP,
       min(pr.PRIORITY) as PRIORITY,
       pia.stat,
       pr.ID_NODE,
       pr.TYP_REF,
       pr.ID_REF_1,
       pr.ID_REF_2,
       pr.ID_REF_3,
       pr.ID_REF_4,
       pr.ID_REF_5,
       pr.ID_REF_6,
       pr.ID_CLIENT,
       pr.ID_ARTICLE,
       sum(pr.QTY) as qty,
       pr.ARTVAR,
       pr.TYP_STOCK,
       pr.TYP_LOCK,
       pr.ID_BATCH,
       pr.TS_BBD,
       pr.TYP_SPECIAL_STOCK,
       pr.ID_SPECIAL_STOCK,
       pr.STAT_QUALITY_CONTROL,
       pr.STAT_CUSTOMS,
       pr.SEP_CRIT_01,
       pr.SEP_CRIT_02,
       pr.SEP_CRIT_03,
       pr.SEP_CRIT_04,
       pr.SEP_CRIT_05,
       pr.SEP_CRIT_06,
       pr.SEP_CRIT_07,
       pr.SEP_CRIT_08,
       pr.SEP_CRIT_09,
       pr.SEP_CRIT_10,
       sum(pr.WT) as wt,
       pr.UNIT_WT,
       sum(pr.VOL) as vol,
       pr.UNIT_VOL,
       pia.ID_LU_SRC,
       pia.ID_QUANTUM_SRC,
       pia.STORAGE_AREA_SRC,
       pia.STORAGE_LOCATION_SRC,
       pia.AISLE_SRC,
       pia.FLG_CLEARING,
       pia.QTY_ACTUAL,
       pia.NUM_SORT,
       pr.store_out_zone_src,
       pio.id_picking_trip,
       pio.id_store_out_zone,
       pio.id_picking_order
  from bps_process_record         pr,
       pi_picking_order           pio,
       pi_picking_order_bps_asgmt pia
 where pr.id_site = pia.id_site
   and pr.id_record = pia.id_record
   and pia.id_site = pio.id_site
   and pia.id_picking_order = pio.id_picking_order
 group by  pr.ID_SITE,
       pia.ID_RECORD,
       pia.id_pick,
       pr.TYP,
       pia.stat,
       pr.ID_NODE,
       pr.TYP_REF,
       pr.ID_REF_1,
       pr.ID_REF_2,
       pr.ID_REF_3,
       pr.ID_REF_4,
       pr.ID_REF_5,
       pr.ID_REF_6,
       pr.ID_CLIENT,
       pr.ID_ARTICLE,
       pr.ARTVAR,
       pr.TYP_STOCK,
       pr.TYP_LOCK,
       pr.ID_BATCH,
       pr.TS_BBD,
       pr.TYP_SPECIAL_STOCK,
       pr.ID_SPECIAL_STOCK,
       pr.STAT_QUALITY_CONTROL,
       pr.STAT_CUSTOMS,
       pr.SEP_CRIT_01,
       pr.SEP_CRIT_02,
       pr.SEP_CRIT_03,
       pr.SEP_CRIT_04,
       pr.SEP_CRIT_05,
       pr.SEP_CRIT_06,
       pr.SEP_CRIT_07,
       pr.SEP_CRIT_08,
       pr.SEP_CRIT_09,
       pr.SEP_CRIT_10,
       pr.UNIT_WT,
       pr.UNIT_VOL,
       pia.ID_LU_SRC,
       pia.ID_QUANTUM_SRC,
       pia.STORAGE_AREA_SRC,
       pia.STORAGE_LOCATION_SRC,
       pia.AISLE_SRC,
       pia.flg_clearing,
       pia.qty_actual,
       pia.NUM_SORT,
       pr.store_out_zone_src,
       pr.ID_LU_TGT,
       pr.STORAGE_AREA_TGT,
       pr.STORAGE_LOCATION_TGT,
       pio.id_picking_trip,
       pio.id_store_out_zone,
       pio.id_picking_order
;
