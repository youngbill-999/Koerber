<A NAME="SQL"></A>
<TABLE CLASS="SUB_TABLE"><TR><TD CLASS="SUB_TITLE">
SQL
</TD></TR></TABLE><PRE CLASS="DECL_TEXT">
create or replace view sls_v_store_in_location as
select
          tsl.id_site || '@' || tsl.storage_area || '@' || tsl.storage_location || '@' || ols.typ_lu || '@' || tia.id_store_in_group || '@' as code_ref,
          tsl.id_site,
          tsl.storage_area,
          tsl.storage_location,
          tsl.flg_fixed_location,
          tsl.stat_occupied,
          tsl.cl_abc,
          case tsl.cl_abc
             when 'A'  then 1
             when 'B'  then 2
             when 'C'  then 3
             else           9
          end as sort_val_for_a,
          case tsl.cl_abc
             when 'B'  then 1
             when 'A'  then 2
             when 'C'  then 3
             else           9
          end as sort_val_for_b,
          case tsl.cl_abc
             when 'C'  then 1
             when 'B'  then 2
             when 'A'  then 3
             else           9
          end as sort_val_for_c,
          tsl.aisle,
          tsl.site_requirement1,
          tsl.site_requirement2,
          tsl.site_requirement3,
          tsl.height,
          tsl.wt_residual,
          tsl.wt_max,
          tsl.num_control,
          tsl.id_store_in_zone,
          tsl.id_store_out_zone,
          tsl.typ_storage_location,
          tsl.num_max_capacity,
          tsl.num_remaining_capacity,
          tsl.flg_single_article,
          tia.id_store_in_group,
          tlt.flg_dyn_ctrl_rem_cap,
          tlt.typ_remaining_capacity,
          tlt.cl_lu,
          ols.typ_lu,
          ols.pct_location_demand,
          ols.priority                as priority,
          tlt.flg_lu_typ_sl_typ,
          'OK'                        as result_check
     from                 top_storage_location      tsl
          left outer join top_store_in_zone         tiz
                             on     tiz.id_site              = tsl.id_site
                                and tiz.id_store_in_zone     = tsl.id_store_in_zone
          left outer join top_store_in_zn_grp_asgmt tia
                             on     tia.id_site              = tsl.id_site
                                and tia.id_store_in_zone     = tsl.id_store_in_zone
          left outer join top_store_in_group        tig
                             on     tig.id_site              = tia.id_site
                                and tig.id_store_in_group    = tia.id_store_in_group
               inner join top_storage_location_type tlt
                             on     tlt.id_site              = tsl.id_site
                                and tlt.typ_storage_location = tsl.typ_storage_location
          left outer join top_storage_aisle         tai
                             on     tai.id_site              = tsl.id_site
                                and tai.aisle                = tsl.aisle
          left outer join om_lut_slt_asgmt          ols
                             on     ols.id_site              = tsl.id_site
                                and ols.typ_storage_location = tlt.typ_storage_location
       -- location is suitable
    where            tsl.flg_block_conv_tec   = '0'
      and            tsl.flg_block_store_in   = '0'
      and            tsl.flg_stock_taking     = '0'
       -- store-in-zone is active (if present)
      and (          tsl.id_store_in_zone  is null
            or       tiz.flg_active           = '1')
       -- store-in-zone-group-assignment is active (if present)
      and (          tsl.id_store_in_zone  is null
            or       tia.id_store_in_group is null
            or       tia.flg_active           = '1')
       -- store-in-group is active (if present)
      and (          tsl.id_store_in_zone  is null
            or    tig.id_store_in_group is null
            or       tig.flg_active           = '1')
       -- aisle is suitable (if present)
      and (          tai.flg_block_store_in  is null
            or       tai.flg_block_store_in   = '0')
      and            tlt.flg_active           = '1'
       -- only active LU-Loc-Assignments (if loc type demands it)
      and (          tlt.flg_lu_typ_sl_typ    = '0'
            or (     tlt.flg_lu_typ_sl_typ    = '1'
                 and ols.flg_active           = '1'))
;
</PRE>
